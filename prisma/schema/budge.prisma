model Budget {
    id           String   @id @default(uuid())
    title        String
    description  String
    budgetAmount Float
    fiscalYear   String
    receiveDate  DateTime
    isDeleted    Boolean  @default(false)

    fundSourceId String
    fundSource   FundSource @relation(fields: [fundSourceId], references: [id])

    projects  Project[]
    createdAt DateTime  @default(now())
    updateAt  DateTime  @updatedAt
}
